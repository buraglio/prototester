{
  "global": {
    "output_file": "/var/log/prototester.log",
    "log_level": "info",
    "default_count": 5,
    "timeout": "3s",
    "interval": "1s",
    "json_output": true,
    "influxdb": {
      "enabled": false,
      "url": "http://localhost:8086",
      "token": "your-influxdb-token",
      "organization": "your-org",
      "bucket": "network-monitoring",
      "measurement": "network_latency",
      "batch_size": 1000,
      "flush_interval": "1s"
    }
  },
  "daemon": {
    "enabled": false,
    "run_interval": "5m",
    "output_file": "/var/log/prototester-daemon.log",
    "log_file": "/var/log/prototester-daemon.log",
    "pid_file": "/var/run/prototester.pid",
    "max_log_size": 104857600,
    "rotate_logs": true,
    "stop_on_failure": false,
    "max_retries": 3,
    "retry_interval": "30s"
  },
  "tests": [
    {
      "name": "Google DNS TCP",
      "type": "tcp",
      "target_ipv4": "8.8.8.8",
      "target_ipv6": "2001:4860:4860::8888",
      "port": 53,
      "count": 10,
      "timeout": "3s",
      "interval": "500ms",
      "enabled": true
    },
    {
      "name": "Cloudflare DNS UDP",
      "type": "udp",
      "target_ipv4": "1.1.1.1",
      "target_ipv6": "2606:4700:4700::1111",
      "port": 53,
      "count": 5,
      "enabled": true
    },
    {
      "name": "Google Ping",
      "type": "icmp",
      "target_ipv4": "8.8.8.8",
      "target_ipv6": "2001:4860:4860::8888",
      "count": 10,
      "size": 64,
      "enabled": true
    },
    {
      "name": "DNS UDP Query",
      "type": "dns",
      "target_ipv4": "8.8.8.8",
      "target_ipv6": "2001:4860:4860::8888",
      "port": 53,
      "dns_protocol": "udp",
      "dns_query": "google.com",
      "count": 10,
      "enabled": true
    },
    {
      "name": "DNS over TLS",
      "type": "dot",
      "target_ipv4": "1.1.1.1",
      "target_ipv6": "2606:4700:4700::1111",
      "port": 853,
      "dns_protocol": "dot",
      "dns_query": "cloudflare.com",
      "count": 3,
      "enabled": true
    },
    {
      "name": "Google Compare",
      "type": "compare",
      "hostname": "google.com",
      "port": 80,
      "count": 5,
      "enabled": true
    }
  ]
}